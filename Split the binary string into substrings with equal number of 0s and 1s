#include <bits/stdc++.h>
using namespace std;
int equal_partition(string s,int l){

int count1=0;
int count2=0;
int cnt=0;
for(int i=0;i<l;i++){
if(s[i]=='0'){
        count1++;
}
else{
        count2++;
}
if(count1==count2){
        cnt++;
}

}
return cnt;


}

int main() {
string s;
cin>>s;
    cout<<equal_partition(s,s.length());
}
