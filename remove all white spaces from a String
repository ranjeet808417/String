#include <iostream>
#include<string>
using namespace std;
int main() {
    string s;
    getline(cin,s);
    int index=s.find(' ');
    while(index!=-1){
       s.erase(index,1);
       index=s.find(' ');

    }
cout<<s;
}
/*------------
